<!DOCTYPE html>
<html>
<head>
  <title>Poksific</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <meta charset="UTF-8">
  <meta name="Keywords" content="Poksific, Diepriye, Pokima, Pri, Videos, Media, Share, Application, Article, Image " />
	<meta name="Description" content="Poksific is a website for sharing media such as videos, articles and images amongst a community." />
	<meta name="author" content="Diepriye Pokima">
	<meta name="application-name" content="Poksific">
</head>
<body>
	<%= render 'layouts/uheader' %>
	<%= render 'layouts/header' %>
	
	<div id="container">
		<% flash.each do |key, value| %>
			<div class="alert alert-<%= key %>"><%= value %></div>
		<% end %>
	
		<%= yield %>
		<%= debug(params) if Rails.env.development? %>
	</div>
	<%= render 'layouts/footer' %>	

</body>
</html>
