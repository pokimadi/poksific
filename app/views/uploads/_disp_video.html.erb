<% count = 1 %>
<% @v_upload.each do |vid| %>
	<% @vid = vid
	if (count == 2) %>
		<% count += 1%>
		<%= render 'uploads/vid_holder' %>
		
	<% elsif count == 1 
		count += 1 %>
		<ul class="thumbnails">
		<%= render 'uploads/vid_holder' %>	
		
	<%else 
		count = 1 %>
		<%= render 'uploads/vid_holder' %>	
		</ul>
		
		
	<% end %>

<% end %>
</ul>